{
    "name": "vuejs-ajax",
    "version": "1.3.5",
    "description": "A light XHR plugin for Vue 2.x and and above versions. It has many similar features with `jQuery`'s `ajax()` and `Angular`'s `http()`. In addition to these, it also has its own practical features. For example, `assets`, `component shifter`, `file upload`, `history`, `title`, and `preventing dublicate request` features.",
    "main": "src/vuejs-ajax.js",
    "author": "Fatih Koca",
    "license": "MIT",
    "directories": {
        "example": "test"
    },
    "scripts": {
        "build": "webpack --config webpack.prod.js",
        "lint": "jshint *.js",
        "test": "mocha test/test.js",
        "start": "webpack-dev-server --open --config webpack.dev.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fattihkoca/vuejs-ajax.git"
    },
    "dependencies": {
        "vue": "^2.5.16"
    },
    "keywords": [
        "vue",
        "ajax",
        "xhr",
        "file-upload",
        "dynamic Vue components",
        "async Vue components",
        "delete",
        "get",
        "head",
        "jsonp",
        "patch",
        "post",
        "put",
        "pjax",
        "pushState"
    ],
    "bugs": {
        "url": "https://github.com/fattihkoca/vuejs-ajax/issues"
    },
    "homepage": "https://github.com/fattihkoca/vuejs-ajax#readme",
    "devDependencies": {
        "clean-webpack-plugin": "^0.1.19",
        "custom-event": "^1.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jsdom": "^11.11.0",
        "jsdom-global": "3.0.2",
        "mocha": "^1.17.1",
        "webpack": "^4.12.0",
        "webpack-cli": "^3.0.8",
        "webpack-command": "^0.2.1",
        "webpack-merge": "^4.1.3",
        "xmlhttprequest": "^1.8.0"
    }
}
